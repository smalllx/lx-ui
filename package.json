{
  "name": "lx-ui-test-demo",
  "version": "0.1.1",
  "description": "基于 vue-cli3 的 UI 组件库",
  "main": "lib/xr-ui.umd.min.js",
  "keywords": ["lx-ui", "lxui", "lxUI"],
